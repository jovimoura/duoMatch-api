{
  "env": {
    "es2021": true,
    "node": true
  },

  "extends": [
    "plugin:prettier/recommended",
    "standard",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "prettier/standard"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "space-before-function-paren": "off"
  }
}
